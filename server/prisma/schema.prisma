generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "env(DATABASE_URL)"
}

model Transaction {
  id          Int     @id @default(autoincrement())
  step        Int
  customer    String
  age         Int
  gender      String
  zipcodeOri  String
  merchant    String
  zipMerchant String
  category    String
  amount      Float
  fraud       Boolean
}
